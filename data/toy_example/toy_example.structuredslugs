[ENV_TRANS]

skill4 & !x0 & x1 & !x2 & !y0 & y1 & !y2 -> ((!x0' & x1' & !x2' & !y0' & !y1' & y2' & waypoint'))
skill5 & !x0 & x1 & !x2 & !y0 & !y1 & y2 -> ((!x0' & !x1' & x2' & !y0' & !y1' & y2' & waypoint'))

[ENV_TRANS_HARD]
